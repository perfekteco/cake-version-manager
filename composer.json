{
	"name": "perfekteco/cake-version-manager",
	"type": "cakephp-plugin",
	"description": "Gestionnaire de version sÃ©mantique pour CakePHP - Application et plugins",
    "keywords": [
        "cakephp",
        "plugin",
        "versioning",
        "semantic versioning"
    ],
	"homepage": "https://github.com/perfekteco/cake-version-manager",
	"license": "MIT",
	"authors": [
		{
			"name": "Alex YONDO",
			"homepage": "https://",
			"role": "Maintainer"
		}
	],
	"require": {
		"php": ">=8.1",
		"ext-json": "*",
		"cakephp/cakephp": "^5.0.0"
	},
	"require-dev": {
		"phpunit/phpunit": "^10.2",
		"fig-r/psr2r-sniffer": "dev-next"
	},
	"support": {
		"source": "https://github.com/perfekteco/cake-version-manager"
	},
	"autoload": {
		"psr-4": {
			"Versioning\\": "src/"
		}
	},
    "config": {
        "allow-plugins": {
            "cakephp/plugin-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
